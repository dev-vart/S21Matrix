#include <check.h>
#include "../s21_matrix.h"

/*
typedef struct matrix_struct {
  double **matrix;
  int rows;
  int columns;
} matrix_t;
*/

#test eq_matrix_invalid_args_1
    int res_exp = 0;  // Ожидаемый результат
    matrix_t a = {S21_NULL, 2, 2};
    matrix_t b;
    s21_create_matrix(2, 2, &b);

    int res_act = s21_eq_matrix(&a, &b);

    ck_assert_int_eq(res_exp, res_act);